<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Repository\experienceRepository" name="AppBundle\Entity\experience">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="type" type="integer" column="type"/>
    <field name="start" type="datetime" column="start"/>
    <field name="end" type="datetime" column="end"/>
    <field name="location" type="string" column="location" length="255"/>
  </entity>
</doctrine-mapping>
